<% @books.each do |book| %>

  <p>Title:</p>
  <span><%= book.title %></span>

  <p>Body</p>
  <span><%= book.body %></span>
  
  
  
  
<% end %>

<h1>New book</h1>
<%= form_with model: @book do |f| %>
  <p>title</p>
  <%= f.text_field :title %>

  <P>Body</P>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>
<% end %>

